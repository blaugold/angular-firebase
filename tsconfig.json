{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "noImplicitAny": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es2015"
    ],
    "target": "es5",
    "types": [
      "node",
      "jasmine",
      "zone.js"
    ]
  },
  "exclude": [
    "node_modules",
    "compiled",
    "release",
    "docs",
    "test",
    "examples"
  ]
}